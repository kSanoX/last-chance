{
    "compilerOptions": {
      "target": "ES5",
      "lib": ["dom", "dom.iterable", "ESNext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "ESNext",
      "moduleResolution": "Node",
      "resolveJsonModule": true,
      "jsx": "preserve",
      "incremental": true
    },
    "include": ["src", "server.js"]
  }
  